[tool.poetry]
name = "{{kebabCase name}}"
version = "0.1.0"
description = "Python package for {{kebabCase name}}"
authors = ["Your Name <you@example.com>"]
license = "MIT"
packages = [{ include = "{{snakeCase name}}" }]

[tool.poetry.dependencies]
python = "^3.11"
fastapi = "^0.115.8"
psutil = "^7.0.0"

[tool.poetry.dev-dependencies]
pytest = "^8.3.4"
pytest-asyncio = "^0.25.3"

[build-system]
requires = ["poetry-core>=1.0.0"]
build-backend = "poetry.core.masonry.api"